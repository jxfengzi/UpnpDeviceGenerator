<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
<specVersion>
	<major>1</major>
	<minor>0</minor>
</specVersion>

<actionList>
	<action>
		<name>RB_setBindState</name>
		<argumentList>
			<argument>
				<name>BindState</name>
				<relatedStateVariable>A_ARG_TYPE_BindState</relatedStateVariable>
				<direction>in</direction>
			</argument>
		</argumentList>
	</action>
</actionList>


<serviceStateTable>
	<!-- -->
	<stateVariable sendEvents="yes">
		<name>RB_BindToken</name>
		<dataType>string</dataType>
		<defaultValue></defaultValue>
	</stateVariable>
	<stateVariable sendEvents="no">
		<name>A_ARG_TYPE_BindState</name>
		<dataType>int</dataType>
		<defaultValue></defaultValue>
	</stateVariable>
</serviceStateTable>
</scpd>
